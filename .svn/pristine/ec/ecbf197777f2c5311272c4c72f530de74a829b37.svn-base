<script>
    var time="<?php echo $time;?>";
    var action='project';
    var randomstr="<?php echo $randomstr;?>";
    var sign=$.md5(action+randomstr+time);
    $(function () {
        $.ajax({
            url: "<?php echo config('interfaceurl')?>/project",
            type: 'GET',
            dataType: 'json',
            data:{ time:time,action:'project', sign:sign},
            success: function (data) {
                //console.log(data);
                if (data.code == 1) {
                    $('.img_header').html('<img  src="' + data.projects.E_HeaderImg + '" width="100%" height="100%"  />');
                    $('.project-title').html(data.projects.E_Name);
                    $('.plan_price').html(data.projects.E_Money);
                    $('.paln_cycle').html(data.projects.E_Cycle);
                    $('.plan_scale').html(data.projects.E_Scale + '%');
                    $('.plan_coupon').html(data.projects.E_Coupon + '%');
                    $('.Digest').html(data.projects.E_Digest);
                    $('.buy_person').html(data.projects.Purchased);
                    $('.has_person').html(data.projects.Surplus);
                    $('.plan_last').html(data.projects.lastday);
                    $('.project_info').attr('href', '/project?project=' + data.projects.E_Code);
                    mui(mui('#progress')).progressbar().setProgress(data.projects.proportion);
                }

            }
        })
        $.ajax({
            url: "<?php echo config('interfaceurl')?>/getSlideItems",
            type: 'GET',
            dataType: 'json',
            data:{type:1,time:time,action:'getSlideItems', sign:$.md5('getSlideItems'+randomstr+time+'1')},
            success: function (data) {
                if (data.code == 1) {
                    console.log(data);
                    var leg=data.slides.length;
                    if (leg>0){
                        var bannar='';
                        $.each(data.slides,function (k,v) {
                            // console.log(k)
                            if (k==0){
                                bannar+= '<div class="mui-slider-item mui-slider-item-duplicate">';
                                bannar+=  '<a href="#">';
                                bannar+=  '<img src="'+ v.image +'">';
                                bannar+=  '</a>';
                                bannar+=   '</div>';
                            }
                            bannar+=   ' <div class="mui-slider-item">';
                            bannar+=  '  <a href="#">';
                            bannar+=   '  <img src="'+ v.image +'">';
                            bannar+=   '   </a>';
                            bannar+=   ' </div>';
                            if (k==leg-1){
                                bannar+=  '   <div class="mui-slider-item mui-slider-item-duplicate">';
                                bannar+=  '  <a href="#">';
                                bannar+=   '   <img src="'+ v.image +'">';
                                bannar+=  '   </a>';
                                bannar+=  '   </div>';
                            }
                        })
                        $('.bannars').html(bannar);
                        mui("#slider").slider({interval: 3000}); 
                    }
                }

            }
        })
    })
</script>
<script>
    mui(mui('#progress')).progressbar().setProgress(100);
