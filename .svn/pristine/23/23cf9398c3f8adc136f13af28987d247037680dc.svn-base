                            bannar+=  '  <a href="#">';
                            bannar+=   '  <img src="'+ v.image +'">';
                            bannar+=   '   </a>';
                            bannar+=   ' </div>';
                            if (k==leg-1){
                                bannar+=  '   <div class="mui-slider-item mui-slider-item-duplicate">';
                                bannar+=  '  <a href="#">';
                                bannar+=   '   <img src="'+ v.image +'">';
                                bannar+=  '   </a>';
                                bannar+=  '   </div>';
                            }
                        })
                        $('.bannars').html(bannar);
                        mui("#slider").slider({interval: 3000});
                    }
                }

            }
        })
        $.ajax({
            url: "<?php echo config('interfaceurl')?>/getSlideItems",
            type: 'GET',
            dataType: 'json',
            data:{type:2,time:time,action:'getSlideItems', sign:$.md5('getSlideItems'+randomstr+time+'2')},
            success: function (data) {
                if (data.code == 1) {
                     // console.log(data);return;
                    var legs=data.slides.length;
                    if (legs>0){
                        var hui='';
                        $.each(data.slides,function (k,v) {
                            // console.log(k)
                            if (k==0){
                                hui+= '<div class="mui-slider-item mui-slider-item-duplicate">';
                                hui+=  '<a href="#">';
                                hui+=  '<img src="'+ v.image +'">';
                                hui+=  '</a>';
                                hui+=   '</div>';
                            }
                            hui+=   ' <div class="mui-slider-item">';
                            hui+=  '  <a href="#">';
                            hui+=   '  <img src="'+ v.image +'">';
                            hui+=   '   </a>';
                            hui+=   ' </div>';
                            if (k==legs-1){
                                hui+=  '   <div class="mui-slider-item mui-slider-item-duplicate">';
                                hui+=  '  <a href="#">';
                                hui+=   '   <img src="'+ v.image +'">';
                                hui+=  '   </a>';
                                hui+=  '   </div>';
                            }
                        })
                        // console.log(bannar);
                        $('.hui').html(hui);
                         mui("#slider1").slider({interval: 3000});
                    }
                }

            }
        })
    })
</script>
