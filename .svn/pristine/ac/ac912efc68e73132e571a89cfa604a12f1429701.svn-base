                                                <img src="__TMPL__/public/assets/images/zixun_icon.png" />
                                                <span>在线客服</span>
                                            </div> -->
                    </div>

                    <div class="btn_block detail_nav_block-bottom" style="width:40%;">
                        <button class="to_plan"> 提交订房需求</button>
                    </div>
                </div>
            </nav>
        </div>
        <!--弹窗  -->
        <div class="mask"></div>
        <div class="booking-pop">
            <div class="booking-layout">
                <div class="booking-pop-img">
                    <img src="__TMPL__/public/assets/images/booking_process.png" />
                </div>
                <div class="booking-pop-tel">400-990-8830</div>
                <div class="booking-pop-hotLine">专家管家订房热线</div>
                <div class="booking-pop-rightNow"><a href="tel:400-990-8830" style="color:#fff;">立即拨打</a></div>
                <div class="booking-pop-wxUnm">
                    <p>或添加管家微信订房：</p>
                    <p> WTYX3399（点击复制）</p>
                </div>
                <div class="booking-pop-cansel">取消</div>
            </div>

        </div>
    </div>

    <script>
        $(document).ready(function () {
            $('.booking-pop').hide();
            $('.talk,.online,.btn_block').click(function () {
                $('.mask').show();
                $('.booking-pop').show();
            });
            $('.booking-pop-cansel').click(function () {
                $('.mask').hide();
                $('.booking-pop').hide();
            });
                var navH = $("#slider").offset().top;
                $(document).scroll(function () {
                    var scroH = $(document).scrollTop();
                    var oKey = $('#key').offset().top - 200;
                    var oCase = $('#case').offset().top - 200;
                    var oGai = $('#gai').offset().top - 200;
                    if (scroH >= navH) {
                        $("#slider").css({
                            "position": "fixed",
                            "top": 0,
                            "background": "#fff"
                        });
                    } else if (scroH < navH) {
                        $("#slider").css({
                            "position": "static"
                        });
                    }
                    if (scroH >= oGai && scroH > oKey) {
                    $(".key_btn").removeClass("mui-active");
                    $('.case_btn').removeClass('mui-active');
                    $('.gai_btn').addClass('mui-active');
                }
                else if (scroH >= oKey && scroH < oGai) {
                    $(".key_btn").addClass("mui-active");
                    $('.case_btn').removeClass('mui-active');
                    $('.gai_btn').removeClass('mui-active');
                }
                else if (scroH >= oCase) {
                    $(".key_btn").removeClass("mui-active");
                    $('.case_btn').addClass('mui-active');
                    $('.gai_btn').removeClass('mui-active');
                }
                })
            $('.mui-segmented-control-inverted>div').click(function () {
                $('html, body').animate({
                    scrollTop: $('#' + $(this).attr('case')).offset().top - 80
                }, 1000);
            })

        });
    </script>
