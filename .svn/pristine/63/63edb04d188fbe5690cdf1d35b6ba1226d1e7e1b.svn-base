<script type="text/javascript">
    var sn="<?php echo $sn;?>"
    var token="<?php echo cookie('token');?>"
    $(function () {


        $.ajax({
            type: 'GET',
            url : 'http://www.iqianclub.com/interface/public/index.php/speed/order/resultorder',
            // url : 'http://localhost:82/speed/order/car',
            dataType: 'jsonp',
            xhrFields:{withCredentials:true},
            data:{token:token,sn:sn},
            success:function(data){
                    alert('成功');
                console.log(data);
                var str='';
                if (data.code=1){
                    str+='支付类型是:'+data.info.E_PayChannel+'<br>';
                    str+='价格:'+(data.info.E_PayPrice)/100+'</span><br>';
                    str+='状态:'
                            if (data.info.E_PayState==1){
                                str+='失败'
                            }else{
                                str+='成功'
                            }
                }
                    $(".tc-main").html(str);

            },

        })

    })


</script>
