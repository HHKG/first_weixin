html {
	font-size: 10px;
}

/* 填写资料页面的优先级样式*/
html body {
	background: #fff;
}
.mui-input-group {
	position: inherit;
}
.mui-input-group .mui-input-row:after{
	position: inherit;
}

.mui-content {
	background: #fff;
}
.mui-btn-block {
	margin-top: 1.5rem;
	font-size: 2rem;
	color: #fff;
	height: 5rem;
	background: #f0753a;
	border-radius: 1rem;
}
.mui-input-group .mui-input-row {
	height: 4rem;
	font-size: 1.5rem;
	line-height: 4.5rem;
}
.mui-checkbox input[type=checkbox]:before,
.mui-radio input[type=radio]:before {
	font-size: 2rem;
	line-height: 2;
	color:#3A8782;
}


/* a {
	font-size: 1.2rem;
	line-height: 1.2rem;
} */

.mui-table-view-cell>a:not(.mui-btn) {
	padding: 1.6rem 1.5rem;
}
.mui-input-group {
	position: inherit;
}
.mui-input-group .mui-input-row {
	height: 4rem;
	font-size: 1.5rem;

}
.select_text{
	color:#666666;
	border-radius: 0.2rem;
}
.select:after{
	display: none;
}
.mui-slider .mui-segmented-control.mui-segmented-control-inverted .mui-control-item.mui-active {
	background-image: url(../images/tixian_btn.png);
	background-repeat: no-repeat;
	background-size: 20%;
	background-position-x: 50%;
	background-position-y: 100%;
	color: #333333;
	line-height: 3rem;
}

.nav_wai {
	height: 7rem;
	display: flex;
	align-items: center;
	border-bottom: 1px solid #C8C8C8;
	border-top: 1px solid #C8C8C8;
}

.mui-segmented-control.mui-segmented-control-inverted .mui-control-item {
	color: #666666;
	font-size: 1.6rem;
}

.float {
	position: fixed;
	top: 0px;
}

.mui-slider-indicator .mui-indicator.mui-active{
	border-radius: 0.4rem;
	width:1.5rem;
	height:0.5rem;
	background:#fff;
	box-shadow:0 0 0px 0px rgba(130,130,130,.7);
}
.mui-slider-indicator .mui-indicator{
	border-radius: 0.4rem;
	width:0.5rem;
	height:0.5rem;
	background:rgba(255,255,255,0.3);
	box-shadow:0 0 0px 0px rgba(130,130,130,.7);
	margin: 0;
}
.slider_bottom{
	padding-bottom: 1.5rem;
}
