.cash_header{
	display: flex;flex-direction: column;width:100%;height:21rem;background:#3A8782;justify-content: space-around;padding:0 1.5rem;
}
.cash_header div{
	display: flex;justify-content: space-between;
}
.cash_header_span1{
	color:rgba(255, 255, 255, 0.6);font-size: 1.5rem;
}
.cash_header_span2{
	color:#fff;font-size: 1.5rem;
}
.cash_header_span3{
	color:#fff;font-size: 7rem;font-family: 'DIN-BoldAlternate';
}
.cash_btn{
	margin:4rem auto;width:92%;
}
.withdraw_btn{
	border:hidden;background:#F58650;height:5rem;width:100%;font-size: 2rem;color:#fff;border-radius: 70px;
}
.credit_card{
	height:5rem;display: flex;justify-content: space-between;align-items: center;align-items: center;padding:0 1.5rem;background-color:#fff;
}
.credit_card_img{
	height:8rem;display: flex;align-items: center;
}
.credit_card_img img{
	width:3rem;height:2rem;
}
.credit_card_img span{
	font-size:2rem;padding-left:1.5rem;
}
.right_icon{
	width:1rem;height:2rem;
}

